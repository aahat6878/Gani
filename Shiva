	WebDriver driver;
	@BeforeMethod
	public void test()
	{
		driver=setupChrome();
		driver.manage().window().maximize();
	}
	@Test(priority=1)
	public void search() throws Exception
	{
		driver.manage().timeouts().implicitlyWait(10,TimeUnit.SECONDS);
		Actions act=new Actions(driver);
		driver.findElement(By.xpath("//li[@class='cbp-hrmenu-tab cbp-hrmenu-tab-13  cbp-has-submeu']")).click();
		driver.findElement(By.xpath("//*[@id='left-column']//*[text()='Accessories']")).click();
		Thread.sleep(2000);
		driver.findElement(By.xpath("//*[@class='select-title expand-more form-control']//*[@class='fa fa-angle-down']")).click();
		driver.findElement(By.xpath("//*[@class='dropdown-menu']//a[4]")).click();
		String str=driver.findElement(By.xpath("//*[@class='h3 product-title']//a[text()='Pawzone Anti-Slip Spliced Flooring For Pets']")).getText();
		System.out.println("first lowest price of product is:" +str);
		String str1=driver.findElement(By.xpath("//*[@content='148.2']")).getText();
		System.out.println("price:" +str1);
		Thread.sleep(2000);
		driver.findElement(By.xpath("//*[@class='select-title expand-more form-control']//*[@class='fa fa-angle-down']")).click();
		driver.findElement(By.xpath("//*[@class='dropdown-menu']//a[5]")).click();
		String str2=driver.findElement(By.xpath("//*[@class='h3 product-title']//*[text()='Savic Andes 7 Pet Carrier 39x27x32 inches']")).getText();
		System.out.println("first highest price of product is:" +str2);
		String str3=driver.findElement(By.xpath("//*[@content='15200']")).getText();
		System.out.println("price:" +str3);
		Thread.sleep(2000);
		//WebElement list=driver.findElement(By.xpath("//*[@class='fa fa-th-list']"));
		//JavascriptExecutor js=(JavascriptExecutor)driver;
		//js.executeScript(list);
		//js.executeScript("driver.findElement(By.xpath(\"//*[@class='col col-auto col-header-left']//div[@id='desktop_logo']\"))")
		WebElement we3=driver.findElement(By.xpath("//*[@class=' js-search-link']"));
		act.moveToElement(we3).click().perform();
		Thread.sleep(2000);
		WebElement we4=driver.findElement(By.xpath("//*[@class='col col-auto col-header-left']//div[@id='desktop_logo']"));
		act.moveToElement(we4).click().perform();
	}
	@AfterMethod
	public void close()
	{
		driver.quit();
	}

}

