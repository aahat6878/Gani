public class mobile extends driversetup {

 WebDriver driver;

 @Test

 public void search() throws InterruptedException {

  //minimum and max price

  WebElement min=driver.findElement(By.xpath("//*[@class='tiker_input mini_finder_input_1']"));

  min.click();

  min.clear();

  min.sendKeys("5000");

  Thread.sleep(1000);

  WebElement max=driver.findElement(By.xpath("//*[@class='tiker_input mini_finder_input_2']"));

  max.click();

  max.clear();

  max.sendKeys("10000");

  Thread.sleep(1000);

  

  //click on find mobiles

  driver.findElement(By.xpath("//*[@class='view_details_btn find_mob mini_finder_btn enabled']")).click();

  Thread.sleep(1000);

  

  //price low to high

  WebElement lth=driver.findElement(By.xpath("//*[@id='price_box']"));

  lth.click();

  Thread.sleep(2000);

  lth.click();

  Thread.sleep(1000);

  String lmobile=driver.findElement(By.xpath("//*[text()='Itel Selfiepro S41']")).getText();

  String lprice=driver.findElement(By.xpath("//*[text()='5,000'][1]")).getText();

  Thread.sleep(1000);

  System.out.println("Lowest Mobile Name= "+lmobile+" Lowest mobile price= "+lprice);

  

  //price high to low

  Thread.sleep(1000);

  lth.click();

  Thread.sleep(1000);

// String hmobile=driver.findElement(By.xpath("//*[@class='a2 star star_width_new']")).getText();

// String hprice=driver.findElement(By.xpath("//*//*[@class='filter-grey-bar']//*[@class='filter-right']//*[text()='10,000'][1]")).getText();

// Thread.sleep(1000);

// System.out.println("highest Mobile Name= "+hmobile+" highest mobile price= "+hprice);

//  

  //logo

  Thread.sleep(1000);

  driver.findElement(By.xpath("//*[@class='logo']//*[@title='#1 Mobile Portal India | 91mobiles.com']")).click();

  

  

  

 }

 @BeforeTest

 public void beforeTest() {

  driver=chromesetup();

  driver.manage().window().maximize();

 }



