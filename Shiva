	  from.click();

	  //from.clear();

	  Thread.sleep(2000);

	  String fr = excelsetup.getcellvalue("Sheet1",0,0);

	  from.sendKeys(fr);

	  //js.executeScript("arguments[0].value = '"+fr+"';", from);

	  Thread.sleep(2000);

	  from.sendKeys(Keys.ENTER);

	  

	  WebElement dest = driver.findElement(By.xpath("//input[@placeholder='To' and @name='or-dest']"));

	  //dest.click();

	  String dt = excelsetup.getcellvalue("Sheet1",0,1);

	  dest.sendKeys(dt);

	  //js.executeScript("arguments[0].value = '"+dt+"';", dest);

	  Thread.sleep(5000);

	  dest.sendKeys(Keys.ENTER);

	  Thread.sleep(2000);

	  driver.findElement(By.xpath("//input[@name='or-depart' and @data-parsley-required='true']"));

	  

	  js.executeScript("window.scrollBy(0,200)");

	  Thread.sleep(2000);

	  driver.findElement(By.xpath("//*[@data-month='11']//a[text()='29']")).click();
	  

	  

	  driver.findElement(By.xpath("//input[@name='or-return' and @data-parsley-flightreturndate='true']")).click();

	  driver.findElement(By.xpath("//*[@data-month='0']//a[text()='8']")).click();

	  driver.findElement(By.name("passenger")).click();

	  List<WebElement> list = driver.findElements(By.xpath("//*[@class='icon-plus']"));

	  list.get(0).click();

	  driver.findElement(By.xpath("//*[@class='passenger-done-blck']//*[text()='Done']")).click();

	  driver.findElement(By.xpath("//*[@type='submit']//*[text()='Search Flight']")).click();

	  

	  

	  

	  

	  

	 }

	  

