package mobile1;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.Test;

public class search extends driversetup{
	WebDriver driver;
	@BeforeMethod
	public void test()
	{
		driver=setupChrome();
		driver.manage().window().maximize();
	}
	@Test(priority=1)
	public void search() throws Exception
		{
		driver.findElement(By.xpath("//*[@class='tiker_input mini_finder_input_1']")).clear();
		driver.findElement(By.xpath("//*[@class='tiker_input mini_finder_input_1']")).sendKeys("5000");
		Thread.sleep(2000);
		driver.findElement(By.xpath("//*[@class='tiker_input mini_finder_input_2']")).clear();
		Thread.sleep(2000);
		driver.findElement(By.xpath("//*[@class='tiker_input mini_finder_input_2']")).sendKeys("10000");
		driver.findElement(By.xpath("//*[@class='view_details_btn find_mob mini_finder_btn enabled']")).click();
		Thread.sleep(2000);
		driver.findElement(By.xpath("//*[@id='price_box']")).click();
		Thread.sleep(2000);
		driver.findElement(By.xpath("//*[@id='price_box']")).click();
		Thread.sleep(2000);
		String str=driver.findElement(By.xpath("//*[text()='Itel A22']")).getText();
		System.out.println("minimumprice of product is:"+str);
		String str1=driver.findElement(By.xpath("//*[@id=\"finderResults\"]/span/div[4]/div[1]/div[2]/div[2]/div/span")).getText();
		System.out.println("price of product is:"+str1);
		Thread.sleep(2000);
		driver.findElement(By.xpath("//*[@id='price_box']")).click();
		Thread.sleep(2000);
		String str3=driver.findElement(By.xpath("//*[text()='Spice Stellar 439']")).getText();
		System.out.println("maximumprice of product is:"+str3);
		String str4=driver.findElement(By.xpath("//*[@id=\"finderResults\"]/span/div[4]/div[1]/div[2]/div[2]/div/span")).getText();
		System.out.println("price of product is:"+str4);
		driver.findElement(By.xpath("(//*[@title='#1 Mobile Portal India | 91mobiles.com'])[1]")).click();
		}
		@AfterMethod
		public void close()
		{
			driver.quit();
		}
	}

