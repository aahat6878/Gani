public class testcase extends driverclass {

 Homepage test;

 WebDriver driver;

 @Test

 public void search() throws IOException, InterruptedException {



  test = new Homepage(driver);

   String dep = excelcode.getcellvalue("Sheet1", 0, 0);

    test.frombutton(dep);

    String dest = excelcode.getcellvalue("Sheet1", 0, 1);

    test.destbutton(dest);

    test.depdate();

    test.retdate();

    test.passenger();

    

    test.search();

    



    test.flight();

    

    

 }

 @BeforeTest

 public void beforeTest() {

  driver=chromesetup();

 }



 @AfterTest

 public void afterTest() {

