//	driver.findElement(By.xpath("//*[@id=\"ctl00_mainContent_view_date1\"]")).click();

 driver.findElement(By.xpath("//*[@id=\"ui-datepicker-div\"]/div[1]/table/tbody/tr[4]/td[5]/a")).click();

