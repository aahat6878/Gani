  //sub category=toys

  driver.findElement(By.xpath("//*[@class='category-sub-menu']//a[text()='Toys']")).click();

  Thread.sleep(1000);

  

  //click on second product

  JavascriptExecutor js=(JavascriptExecutor)driver;

  js.executeScript("window.scrollBy(0,1000)");

  driver.findElement(By.xpath("//*[@class='product-description']//*[text()='N & D Low Grain starter puppy 800 gm']")).click();

  Thread.sleep(1000);

  

  //Proceed add to cart and checkout

  driver.findElement(By.xpath("//*[@data-button-action='add-to-cart']")).click();

  Thread.sleep(1000);

  

// TakesScreenshot scrshot=(TakesScreenshot)driver;

// File srcFile=scrshot.getScreenshotAs(OutputType.FILE);

// FileUtils.copyFile(srcFile, "C:\\\\Users\\\\J.Siddayya swami\\\\eclipse-workspace\\\\Selenium_SBA\\\\marshall.png");

// driver.findElement(By.xpath("//*[@class='btn btn-primary btn-block btn-lg mb-2'][2]")).click();

  driver.navigate().back();

  Thread.sleep(1000);

  driver.navigate().back();

  

  //print first name and price

  String name=driver.findElement(By.xpath("//*[text()='Vet life dog Ultrahypo 2kg']")).getText();

  String price=driver.findElement(By.xpath("//*[text()='â‚¹1,155.00']")).getText();

  System.out.println("first name= "+name);

  System.out.println("price= "+price);

  

  

  //print the name of first product

  String firstname=driver.findElement(By.xpath("//*[@alt='Versele Laga Junior OM NO Maize 25 Kg']")).getText();

  System.out.println(firstname);

  excelclass.setcellvalue("Sheet1", 0, 0, firstname);

  

  

  

  

 }

