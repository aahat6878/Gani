package cleartrip;

import org.testng.annotations.Test;



//import goindigo.pom.excelsetup;



import org.testng.annotations.BeforeTest;

import java.io.IOException;

import java.util.List;



import org.openqa.selenium.By;

import org.openqa.selenium.Keys;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.testng.annotations.AfterTest;

public class testng extends driversetup {

  WebDriver driver;

  

  @BeforeTest

  public void beforeTest() {

    

   driver=chromesetup();

   driver.manage().window().maximize();

  }

  @Test

  public void f() throws IOException, InterruptedException {

    

     

 String dt = excelsetup.getcellvalue("Sheet1",0,0);

 String dt1 = excelsetup.getcellvalue("Sheet1",0,1);

    driver.findElement(By.xpath("(//input[@class='tripType'])[2]")).click();

    Thread.sleep(2000);

    WebElement txt= driver.findElement(By.xpath("//input[@id = 'FromTag']"));

    txt.sendKeys(dt);

    Thread.sleep(5000);

    txt.sendKeys(Keys.ARROW_DOWN);

    txt.sendKeys(Keys.ENTER);

    Thread.sleep(5000);

    txt =driver.findElement(By.xpath("//input[@id = 'ToTag']"));

    txt.sendKeys(dt1);

    Thread.sleep(5000);

    txt.sendKeys(Keys.ARROW_DOWN);

    txt.sendKeys(Keys.ENTER);

    Thread.sleep(4000);

    List <WebElement> ls=driver.findElements(By.xpath("(//table[@class='calendar'])[1]//td")) ;

    for(WebElement d1:ls){

      String date=d1.getText();

      if(date.equals("17")){

        d1.click();

        break;

      }

    }

    Thread.sleep(2000);

    List<WebElement> ls1=driver.findElements(By.xpath("(//table[@class='calendar'])[2]//td"));

    for(WebElement d2:ls1){

      String date1=d2.getText();

      if(date1.equals("25")){

        d2.click();

        break;

      }

    }

    Thread.sleep(2000);

    driver.findElement(By.xpath("//input[@class='booking large']")).click();

  }

  @AfterTest

  public void afterTest() {

   driver.quit();



  }

}





