import org.openqa.selenium.By;

import org.openqa.selenium.Keys;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.support.ui.Select;

import org.testng.annotations.AfterClass;

import org.testng.annotations.BeforeClass;

import org.testng.annotations.Test;



import Browser.Drivers;

import Input.Excelinput;

import Xpath.Path;



public class Scarching extends Drivers

{



 WebDriver driver;



 @BeforeClass



 public void beforeclass()

 {

  driver= getchromedriver();



 }

 @Test

 public void marshallpetzone() throws Exception

 {

  /*WebElement wb=driver.findElement(By.xpath(Path.thanks));

  wb.click();*/

 WebElement wb1=driver.findElement(By.xpath(Path.pet));

 wb1.click();

 WebElement wb2=driver.findElement(By.xpath(Path.dog));

 wb2.click();

 WebElement wb3=driver.findElement(By.xpath(Path.food));

 wb3.click();

 WebElement wb4=driver.findElement(By.xpath(Path.subcatogory));

 wb4.click();

 /*WebElement wb5=driver.findElement(By.xpath(Path.atoz));

 wb5.click();

   wb5.sendKeys(Keys.DOWN);*/

   Select wb5=new Select(driver.findElement(By.xpath(Path.atoz)));

   //wb5.selectByVisibleText("Name A-Z");]



 wb5.selectByIndex(2);

 Thread.sleep(2000);



 WebElement wb6=driver.findElement(By.xpath(Path.secondpro));



 String itemname = wb6.getText();

 System.out.println("second product name" + itemname );



 WebElement wb7=driver.findElement(By.xpath(Path.secondprice));

 String itemprice = wb7.getText();



 System.out.println("second product price" + itemprice );

 Thread.sleep(2000);

 WebElement wb8=driver.findElement(By.xpath(Path.underprice));

 wb8.click();

 Thread.sleep(2000);

 WebElement wb9=driver.findElement(By.xpath(Path.firstproduct));

 wb9.click();

 Thread.sleep(2000);

 String firstitemname = wb9.getText();

 System.out.println("first product name" + firstitemname );

 Excelinput mne = new Excelinput();

 mne.excel(firstitemname);

}

 @AfterClass

 public void quit()

 {

  driver.close();

 }

